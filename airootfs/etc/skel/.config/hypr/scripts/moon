#!/bin/bash
y=$1
x=$(curl wttr.in/?format=%m)
if [ $x == ðŸŒ‘ ]; then
  echo ðŸŒ•
  echo "8quart" &> ~/.cache/moon
  echo "ðŸŒ•" &> ~/.cache/moone
  exit
elif [ $x == ðŸŒ• ]; then
  echo ðŸŒ‘
  echo "4quart" &> ~/.cache/moon
  echo "ðŸŒ‘" &> ~/.cache/moone
  exit
elif [ $x == ðŸŒ’ ]; then
  echo ðŸŒ–
  echo "7quart" &> ~/.cache/moon
  echo "ðŸŒ–" &> ~/.cache/moone
  exit
elif [ $x == ðŸŒ˜ ]; then 
  echo ðŸŒ”
  echo "1quart" &> ~/.cache/moon
  echo "ðŸŒ”" &> ~/.cache/moone
  exit
elif [ $x == ðŸŒ“ ]; then
  echo ðŸŒ—
  echo "6quart" &> ~/.cache/moon
  echo "ðŸŒ—" &> ~/.cache/moone
  exit
elif [ $x == ðŸŒ— ]; then
  echo ðŸŒ“
  echo "2quart" &> ~/.cache/moon
  echo "ðŸŒ“" &> ~/.cache/moone
  exit
elif [ $x == ðŸŒ” ]; then
  echo ðŸŒ˜
  echo "5quart" &> ~/.cache/moon
  echo "ðŸŒ˜" &> ~/.cache/moone
  exit
elif [ $x == ðŸŒ– ]; then
  echo ðŸŒ’
  echo "3quart" &> ~/.cache/moon
  echo "ðŸŒ’" &> ~/.cache/moone
  exit
else
  cat ~/.cache/moone
  exit
fi
